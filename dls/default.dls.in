#!/bin/sh

dls_dir="@libdir@/dillo/dls"
dls_available=`ls $dls_dir | sed 's/\.dls$//g' | awk '{ print "dpi:" $1 "<br/>" }'`

cat << EOD
HTTP/1.1 200 OK
Content-Type: text/html; charset=UTF-8

<head>
<title>AVAILABLE DLS</title>
</head>

<body>

<h1>AVAILABLE DLS</h1>
<em>Time: `date`</em>

<hr/>

$dls_available

</body>
EOD
